<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DropDownMenu">

        <attr name="ddunderlineColor" format="color" />
        <attr name="dddividerColor" format="color" />
        <attr name="ddtextSelectedColor" format="color" />
        <attr name="ddtextUnselectedColor" format="color" />
        <attr name="ddmenuBackgroundColor" format="color" />
        <attr name="ddmaskColor" format="color" />
        <attr name="ddmenuTextSize" format="dimension" />
        <attr name="ddmenuSelectedIcon" format="reference" />
        <attr name="ddmenuUnselectedIcon" format="reference" />
        <attr name="ddmenuOtherIcon" format="reference" />
        <attr name="ddmenuOtherselectedIcon" format="reference" />
        <attr name="ddmenuMenuHeightPercent" format="float" />
    </declare-styleable>

    <declare-styleable name="StatusBarHeightView">
        <attr name="use_type" format="integer">
            <enum name="use_height" value="0" />
            <enum name="use_padding_top" value="1" />
        </attr>
    </declare-styleable>



    <declare-styleable name="ExpansionHeader">
        <attr name="expansion_layout" format="reference"/>
        <attr name="expansion_toggleOnClick" format="boolean"/>
        <attr name="expansion_headerIndicator" format="reference"/>
        <attr name="expansion_headerIndicatorRotationExpanded" format="integer"/>
        <attr name="expansion_headerIndicatorRotationCollapsed" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ExpansionLayout">
        <attr name="expansion_expanded" format="boolean"/>
    </declare-styleable>

    <attr name="expansion_openOnlyOne" format="boolean"/>

    <declare-styleable name="ExpansionsViewGroupFrameLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupLinearLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupRelativeLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
    <declare-styleable name="ExpansionsViewGroupConstraintLayout">
        <attr name="expansion_openOnlyOne"/>
    </declare-styleable>
</resources>